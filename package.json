{
  "name": "conventional-changelog-cmyr-config",
  "version": "2.1.2",
  "description": "This preset extends the angular preset",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 -n ./index.js",
    "release": "semantic-release",
    "test-windows": "mocha --timeout 30000",
    "deploy-beta": "npm publish --tag beta",
    "deploy": "npm publish",
    "lint": "cross-env NODE_ENV=production eslint *.js --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-cmyr"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "conventional-changelog",
    "angular",
    "preset",
    "changelog",
    "emoji"
  ],
  "author": "CaoMeiYouRen <caomeiyouren@gmail.com>",
  "contributors": [
    "CaoMeiYouRen <caomeiyouren@gmail.com>",
    "Zeng Shunhao <shunhaozeng@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/conventional-changelog-cmyr-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash": "^4.14.178",
    "better-than-before": "^1.0.0",
    "chai": "^4.2.0",
    "commitizen": "^4.3.1",
    "commitlint-config-cmyr": "1.0.0-beta.2",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-changelog-core": "^3.2.2",
    "coveralls": "^3.0.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog-cmyr": "2.0.0-beta.2",
    "eslint": "^9.34.0",
    "eslint-config-cmyr": "2.0.0-beta.15",
    "git-dummy-commit": "^1.3.0",
    "github-url-from-git": "^1.5.0",
    "husky": "^9.1.7",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.10.2",
    "lint-staged": "^16.1.5",
    "mocha": "*",
    "semantic-release": "^18.0.1",
    "shelljs": "^0.8.3",
    "through2": "^3.0.1",
    "typescript": "^5.9.2"
  },
  "changelog": {
    "language": "zh"
  },
  "dependencies": {
    "compare-func": "^2.0.0",
    "debug": "^4.1.1",
    "q": "^1.5.1"
  }
}
